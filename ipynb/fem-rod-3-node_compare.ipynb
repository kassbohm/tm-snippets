        "from sympy.physics.units import *\n", 
        "from sympy import *\n", 
        "\n", 
        "l, E, A, q = var(\"l, E, A, q\")\n", 
        "\n", 
        "x = var(\"x\")\n", 
        "\n", 
        "S = q*(l-x)\n", 
        "sigma = S/A\n", 
        "\n", 
        "pprint(\"\\nu2:\")\n", 
        "u1 = 1/E * integrate(sigma, (x, 0, l/2))\n", 
        "tmp = u1\n", 
        "pprint(tmp)\n", 
        "\n", 
        "pprint(\"\\nu2:\")\n", 
        "u2 = 1/E * integrate(sigma, (x, 0, l))\n", 
        "tmp = u2\n", 
        "pprint(tmp)\n"
      ], 
